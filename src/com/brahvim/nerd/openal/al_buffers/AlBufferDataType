package com.brahvim.nerd.openal.al_buffers;

import org.lwjgl.openal.AL11;

/**
 * @deprecated ...because I won't wrap every single type of constant and make my program's 
 * <i>developement</i> and <b>runtime</b>. slower, y'know!
 */
@Deprecated
public enum AlBufferDataType {
	AL_FORMAT_MONO8(AL11.AL_FORMAT_MONO8),
	AL_FORMAT_MONO16(AL11.AL_FORMAT_MONO16),
	AL_FORMAT_STEREO8(AL11.AL_FORMAT_STEREO8),
	AL_FORMAT_STEREO16(AL11.AL_FORMAT_STEREO16);

	private final int alEnumValue;

	private AlBufferDataType(int p_alEnumValue) {
		this.alEnumValue = p_alEnumValue;
	}

	public int getAlEnum() {
		return this.alEnumValue;
	}
}